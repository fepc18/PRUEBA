@model IEnumerable<Modelo.Inventario.UbicacionDestino>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Revisión de Planta Física</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UbicacionDestinoId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UbicacionDestinoId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Codigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.ActionLink("Registrar Revisión", "Create", new { id=item.UbicacionDestinoId })
            
        </td>
    </tr>
}

</table>
